<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Dashboardparalella" thread-count="2" parallel="tests">
<listeners>
<listener class-name="com.qa.orangehrm.listeners.AllureListenerUtility"/>
<listener class-name="com.aventstack.chaintest.plugins.ChainTestListener"/>
<listener class-name="com.qa.orangehrm.listeners.RetryTransformation"/>
</listeners>
  <test  name="DashboardChrome">
  <parameter name="xmlBrowserName" value="Chrome"></parameter>
    <classes>
      <class name="com.qa.orangehrm.testcases.DashboardPageTest"/>
    </classes>
  </test> <!-- Test -->
  <test  name="DashboarFirefox">
  <parameter name="xmlBrowserName" value="Firefox"></parameter>
    <classes>
      <class name="com.qa.orangehrm.testcases.DashboardPageTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
